<div class="container-fluid">
    <h5>
        <strong>Related Artists</strong>
    </h5>
    <div>
        {{#each relatedArtists.artists}}
        <div class="margin-top-1">
            <img src="{{this.images.2.url}}" class="album-image image-thumbnail" />
        </div>
        <div class="margin-top-point-1">
            <a href="/artist/{{this.id}}" class="link">{{this.name}}</a>
        </div>
        {{/each}}
    </div>
</div>